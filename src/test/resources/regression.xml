<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Regression" verbose="1">
    <test name="Regression">
        <classes>
            <class name="com.phonebook.tests.HomePageTests"/>
            <class name="com.phonebook.tests.HeaderTest"/>
            <class name="com.phonebook.tests.LoginTests">
                <methods>
                    <exclude name=".*Negative.*"></exclude>
                </methods>
            </class>
            <class name="com.phonebook.tests.CreateContactTest">
                <methods>
                    <exclude name=".*From."></exclude>
                </methods>
            </class>
            <class name="com.phonebook.tests.RemoveContactTest"/>
        </classes>
    </test>

</suite>